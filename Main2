python
import time
import random

# Read proxy list from file
with open("proxy.txt", "r") as proxy_file:
    proxies = proxy_file.read().splitlines()

# Read keyword list from file
with open("keyword.txt", "r", encoding="utf-8") as keyword_file:
    keywords = keyword_file.read().strip().splitlines()

chrome_driver_path = "chromedriver"

chrome_options = Options()
chrome_options.add_argument("--user-agent=Mozilla/5.0 (Linux; Android 10; Pixel 3) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/87.0.4280.66 Mobile Safari/537.36")

for proxy in proxies:
    print("Using proxy:", proxy)

    # Set proxy for ChromeOptions
    chrome_options.add_argument('--proxy-server={}'.format(proxy))

    # Create a new WebDriver instance with the updated ChromeOptions
    driver = webdriver.Chrome(executable_path=chrome_driver_path, options=chrome_options)

    driver.maximize_window()

    root = "https://www.google.com/"
    url = "https://google.com/search?q="

    for keyword in keywords:
        query = urllib.parse.quote_plus(keyword)
        link = url + query
        driver.get(link)

        num = '1'
        wait = WebDriverWait(driver, 15)
        wait.until(EC.presence_of_all_elements_located((By.XPATH, '//*[@id="tads"]/div[1]/div/div[1]/div/div[1]/a')))
        headings = driver.find_elements_by_css_selector('div.v5yQqb.jqWpsc')

        for heading in headings:
            title = heading.find_elements_by_tag_name('h3')
            element = heading.find_element_by_css_selector('a')
            data_rw = re.search(r'data-rw="([^"]+)"', element.get_attribute("outerHTML"))
            href = re.search(r'href="([^"]+)"', element.get_attribute("outerHTML"))

            if data_rw and href:
                data_rw_value = data_rw.group(1)
                href_value = href.group(1)

                data_rw_value = data_rw_value.replace("amp;", "")

                print(data_rw_value)

                # Open the URL
                driver.execute_script('''window.open("{}","_blank");'''.format(href_value))
                driver.switch_to.window(driver.window_handles[-1])

                # Scroll to the end of the webpage
                driver.execute_script("window.scrollTo(0, document.body.scrollHeight);")

                # Wait for 1 minute
                time.sleep(60)

                # Close the current tab and switch to the next URL
                driver.close()
                driver.switch_to.window(driver.window_handles[0])

        # Wait for a random duration between 1 and 5 seconds before searching for the next keyword
        time.sleep(random.randint(1, 5))

    # Close the WebDriver instance for the current proxy
    driver.quit()
