python
import tkinter as tk
from tkinter import filedialog

def run_script():
    stay = int(stay_entry.get())
    trynumb = int(try_entry.get())
    user_agent_type = user_agent_var.get()

    chrome_options = Options()
    if user_agent_type == 1:
        chrome_options.add_argument("--user-agent=Mozilla/5.0 (Linux; Android 10; Pixel 3) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/87.0.4280.66 Mobile Safari/537.36")
    elif user_agent_type == 2:
        chrome_options.add_argument("--user-agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/86.0.4240.198 Safari/537.36")

    driver = webdriver.Chrome(service=service, options=chrome_options)
    driver.maximize_window()

    # Rest of the code...

# Create the main window
root = tk.Tk()
root.title("Web Scraping Script")
root.geometry("400x300")

# Stay on each page
stay_label = tk.Label(root, text="Time to stay on each page (seconds):")
stay_label.pack()
stay_entry = tk.Entry(root)
stay_entry.pack()

# Number of tries per proxy
try_label = tk.Label(root, text="Number of tries per proxy:")
try_label.pack()
try_entry = tk.Entry(root)
try_entry.pack()

# User agent type
user_agent_label = tk.Label(root, text="User agent type:")
user_agent_label.pack()
user_agent_var = tk.IntVar()
user_agent_radio1 = tk.Radiobutton(root, text="Mobile", variable=user_agent_var, value=1)
user_agent_radio1.pack()
user_agent_radio2 = tk.Radiobutton(root, text="Desktop", variable=user_agent_var, value=2)
user_agent_radio2.pack()

# Run button
run_button = tk.Button(root, text="Run Script", command=run_script)
run_button.pack()

root.mainloop()
